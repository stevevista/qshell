{
  "name": "qshell",
  "version": "1.0.0",
  "description": "electron desktop bulid demo",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node .electron/dev-runner.js",
    "build": "node .electron/build.js && electron-builder --arch=ia32",
    "build:dir": "node .electron/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron/build.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron/webpack.renderer.config.js",
    "rebuild": "electron-rebuild --arch=ia32",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevevista/qshell.git"
  },
  "author": "R.J.",
  "license": "MIT",
  "build": {
    "asar": true,
    "productName": "qshell",
    "appId": "org.jlq.qshell_sdk",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "res/icons/icon.icns"
    },
    "win": {
      "icon": "res/icons/icon.ico"
    },
    "linux": {
      "icon": "res/icons"
    }
  },
  "bugs": {
    "url": "https://github.com/stevevista/qshell/issues"
  },
  "homepage": "https://github.com/stevevista/qshell#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.1",
    "del": "^3.0.0",
    "electron": "^2.0.11",
    "electron-builder": "^20.28.4",
    "electron-debug": "^2.0.0",
    "electron-rebuild": "^1.8.2",
    "eslint": "^5.7.0",
    "less": "^3.8.1",
    "webpack": "^4.20.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-hot-middleware": "^2.24.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.3",
    "multispinner": "^0.2.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "node-loader": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "url-loader": "^1.1.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "yml-loader": "^2.1.0"
  },
  "dependencies": {
    "adbkit": "^2.11.0",
    "antd": "^3.10.1",
    "electron-log": "^2.2.17",
    "prop-types": "^15.6.2",
    "qia": "github:stevevista/qia",
    "qmsl": "github:stevevista/qmsl",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-intl": "^2.7.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.1",
    "serialport": "^7.0.2",
    "d3": "^5.7.0",
    "filesize": "^3.6.1",
    "fs-extra": "^7.0.0",
    "lodash": "^4.17.11",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "moment": "^2.22.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-touch": "^2.0.0-beta.4",
    "vuex": "^3.0.1"
  }
}
